\ProvidesPackage{beamercolorthemeSCS}
  [2010/05/16 v1.0 beamer 'SCS' color theme by MM]

\mode<presentation>

\definecolor{SCSlightblue}{RGB}{79,130,195}
\definecolor{SCSdark}{RGB}{1,1,1}
\definecolor{SCSwhite}{RGB}{255,255,255}
\definecolor{SCSblue}{RGB}{0,76,103}
\definecolor{SCSgrey}{RGB}{172,160,149}
\definecolor{SCSorange}{RGB}{255,160,35}

\setbeamercolor{normal text}{fg=SCSdark}
\setbeamercolor{alerted text}{fg=SCSorange}
\setbeamercolor{example text}{fg=SCSblue}
\setbeamercolor{structure}{fg=SCSblue}

\setbeamercolor{author}{fg=SCSdark}
\setbeamercolor{date}{fg=SCSgrey}
\setbeamercolor{framesubtitle}{fg=SCSgrey}
\setbeamercolor{title}{fg=SCSlightblue}
\setbeamercolor{subtitle}{fg=SCSdark}
\setbeamercolor{institute}{fg=SCSdark}
\setbeamercolor{frametitle}{fg=SCSwhite}

\mode<all>

